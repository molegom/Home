@model HomeWeb.Models.FlatViewModel

@{
    ViewBag.Title = "Flat ";
    //Layout = "_Layout";
}
<link rel="stylesheet" type="text/css" href="../../Slick/slick.css" />
@*// Add the new slick-theme.css if you want the default styling*@
<link rel="stylesheet" type="text/css" href="../../Slick/slick-theme.css"/>
<script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
<script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
<script type="text/javascript" src="../../Slick/slick.css"></script>
@Html.ActionLink("Home/FlatList", "Index", "Flat")
<br />
<h2>@Model.Flat.Name</h2>

<div class="mainCurrentFlatDiv">
    @Model.Flat.Address
    <br />
    @Model.Flat.Id
    <br />
    @Model.Flat.BuyDate
    <br />
    @Model.Flat.ComplexName
    <br />
    @Model.Flat.CreatedDate
    <br />
    @{
        string numberFlat = @Model.Flat.NumberFlat.HasValue ? @Model.Flat.NumberFlat.Value.ToString() : string.Empty;
    }
    <span>@numberFlat</span>
    <br />
    @Model.Flat.Status
    <br/>
    @* Show photos *@
    <div class="your-class">
        <div>222</div>
        <div>333</div>
        <div>444</div>
    </div>

    <script type="text/javascript" language="javascript">
        $(document).ready(function () {
            $('.your-class').slick({
                // setting-name: setting-value
            });
        });
    </script>
</div>

