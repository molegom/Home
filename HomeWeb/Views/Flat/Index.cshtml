@model HomeWeb.Models.FlatListViewModel

@{
    ViewBag.Title = "Flat List";
    //Layout = "_Layout";
}
<h2>title</h2>
<div>
    @{
        int widthCount = 10;
        for (int i = 0; i <= @Model.Flats.Count / widthCount; i++)
        {
            <div style="width: 100px; height: 100px; border: solid 1px black;">@Model.Flats[i].Name</div>
        }
    }
</div>

